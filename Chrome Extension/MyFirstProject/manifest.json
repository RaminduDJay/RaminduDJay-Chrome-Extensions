{
    "manifest_version": 2,
    "name": "My First Extension",
    "version": "1.0",
    "description": "A simple Chrome Extension",
    "icons": {
        "48": "icon.png"
    },
    "browser_action": {
        "default_icon": "Icon.png",
        "default_popup": "Popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}